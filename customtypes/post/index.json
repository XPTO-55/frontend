      },
      "slices": {
        "type": "Slices",
        "fieldset": "Slice Zone",
        "config": {
          "choices": {
            "text": {
              "type": "SharedSlice"
            },
            "quote": {
              "type": "SharedSlice"
            },
            "image": {
              "type": "SharedSlice"
            }
          }
        }
      }
    }
  }
}